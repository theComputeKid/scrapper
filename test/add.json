{
	"$schema": "https://raw.githubusercontent.com/theComputeKid/scrapper/main/utils/schema.json",
	"linkage-macro": "SCRAPPER",
	"includes": [
		"<stdint.h>"
	],
	"functions": [
		{
			"name": "add",
			"combination": "all",
			"description": "Adds two numbers.",
			"templates": [
				{
					"name": "S",
					"types": [
						"uint32_t",
						"int8_t"
					]
				},
				{
					"name": "T",
					"types": [
						"float",
						"double"
					]
				}
			],
			"parameters": [
				{
					"name": "left",
					"type": "S",
					"const": false,
					"pointer": false,
					"description": "Left argument to add."
				},
				{
					"name": "right",
					"type": "T",
					"const": false,
					"pointer": false,
					"description": "Right argument to add."
				}
			],
			"return": {
				"type": "T",
				"const": false,
				"pointer": false,
				"description": "Result of the addition."
			}
		}
	]
}
