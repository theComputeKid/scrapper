                    "const": true,
                    "pointer": false
                },
                {
                    "name": "right",
                    "type": "T",
                    "const": true,
                    "pointer": false
                }
            ],
            "return": {
                "type": "T",
                "const": false,
                "pointer": false
            }
        },
        {
            "name": "divide",
            "combination": "fixed",
            "templates": [
                {
                    "name": "T1",
                    "types": [
                        "float",
                        "int8_t"
                    ]
                },
                {
                    "name": "T2",
                    "types": [
                        "double",
                        "int16_t"
                    ]
                }
            ],
            "parameters": [
                {
                    "name": "left",
